<head>
  <title><%= pageTitle %></title>
  <link rel="stylesheet" href="/styles.css" />
  <link rel="stylesheet" href="/home-v3.css" />
  <% if (locals.additionalStyles) { %>
    <% additionalStyles.forEach(function(style) { %>
      <link rel="stylesheet" href="<%= style %>" />
    <% }); %>
  <% } %>
  <link rel="preconnect" href="https://fonts.googleapis.com" />
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
  <link href="https://fonts.googleapis.com/css2?family=Orbitron:wght@400;600;700;900&family=<%= fontFamily %>&display=swap"
    rel="stylesheet" />
  <script src="/browser-tab-id.js"></script>
  <% if (locals.script) { %>
    <script src="<%= script %>" defer></script>
  <% } %>
</head>
